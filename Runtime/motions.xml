<?xml version="1.0" encoding="utf-8"?>
<ArrayOfAnimationDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- New game selection -->

  <AnimationDefinition EntryEvents="GameActivate" Panels="0" NextAnimation="CycleForwardWait" ExitEvents="GameDeactivate(throw), PanelDeactivate(CycleBack)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="220">
      <Position0 X="0" Y="100" Z="-300" />
      <Position1 X="0" Y="100" Z="-300" />
      <Position2 X="0" Y="0" Z="-300" />
      <Position3 X="0" Y="0" Z="0" />
      <Rotation0 X="90" Y="0" Z="0" />
      <Rotation1 X="90" Y="0" Z="0" />
      <Rotation2 X="60" Y="0" Z="0" />
      <Rotation3 X="19.3" Y="0" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <AnimationDefinition EntryEvents="GameActivate" Panels="1,2,3,4" NextAnimation="PanelIn" ExitEvents="GameDeactivate(), PanelActivate(CycleForward)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="220">
      <Position0 X="-600" Y="0" Z="0" />
      <Position1 X="-600" Y="0" Z="0" />
      <Position2 X="-600" Y="0" Z="0" />
      <Position3 X="-600" Y="0" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <!-- Bring in the remaining panels -->
  
  <AnimationDefinition Name="PanelIn" Panels="1" NextAnimation="CycleBackWait" ExitEvents="GameActivate(throw), GameDeactivate(throw), PanelActivate(CycleForward)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="500">
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Position0 X="-285" Y="0" Z="400" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="-85" Y="0" Z="400" />
      <Scale0 X="0.5" Y="1" Z="1" />
      <Scale1 X="0.5" Y="1" Z="1" />
      <Scale2 X="0.5" Y="1" Z="1" />
      <Scale3 X="0.5" Y="1" Z="1" />
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="0" Z="0" />
      <Rotation2 X="0" Y="0" Z="0" />
      <Rotation3 X="0" Y="0" Z="0" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="*.33" Z="0" />
      <Rotation2 X="0" Y="*.66" Z="0" />
      <Rotation3 X="0" Y="-20" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <AnimationDefinition Name="PanelIn" Panels="2" NextAnimation="CycleBackWait" ExitEvents="GameActivate(throw), GameDeactivate(throw), PanelActivate(CycleForward)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="500">
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Position0 X="285" Y="0" Z="400" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="85" Y="0" Z="400" />
      <Scale0 X="0.5" Y="1" Z="1" />
      <Scale1 X="0.5" Y="1" Z="1" />
      <Scale2 X="0.5" Y="1" Z="1" />
      <Scale3 X="0.5" Y="1" Z="1" />
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="0" Z="0" />
      <Rotation2 X="0" Y="0" Z="0" />
      <Rotation3 X="0" Y="0" Z="0" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="*.33" Z="0" />
      <Rotation2 X="0" Y="*.66" Z="0" />
      <Rotation3 X="0" Y="20" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <AnimationDefinition Name="PanelIn" Panels="3" NextAnimation="CycleBackWait" ExitEvents="GameActivate(throw), GameDeactivate(throw), PanelActivate(CycleForward)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="500">
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Position0 X="-390" Y="0" Z="220" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="-195" Y="0" Z="260" />
      <Scale0 X="0.5" Y="1" Z="1" />
      <Scale1 X="0.5" Y="1" Z="1" />
      <Scale2 X="0.5" Y="1" Z="1" />
      <Scale3 X="0.5" Y="1" Z="1" />
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="0" Z="0" />
      <Rotation2 X="0" Y="0" Z="0" />
      <Rotation3 X="0" Y="0" Z="0" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="*.33" Z="0" />
      <Rotation2 X="0" Y="*.66" Z="0" />
      <Rotation3 X="0" Y="-70" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <AnimationDefinition Name="PanelIn" Panels="4" NextAnimation="CycleBackWait" ExitEvents="GameActivate(throw), GameDeactivate(throw), PanelActivate(CycleForward)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="500">
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Position0 X="390" Y="0" Z="220" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="195" Y="0" Z="260" />
      <Scale0 X="0.5" Y="1" Z="1" />
      <Scale1 X="0.5" Y="1" Z="1" />
      <Scale2 X="0.5" Y="1" Z="1" />
      <Scale3 X="0.5" Y="1" Z="1" />
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="0" Z="0" />
      <Rotation2 X="0" Y="0" Z="0" />
      <Rotation3 X="0" Y="0" Z="0" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="200">
      <Rotation0 X="0" Y="0" Z="0" />
      <Rotation1 X="0" Y="*.33" Z="0" />
      <Rotation2 X="0" Y="*.66" Z="0" />
      <Rotation3 X="0" Y="70" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <!-- Send a panel forward -->

  <AnimationDefinition Name="CycleForward" Panels="0" NextAnimation="CycleForwardWait" ExitEvents="PanelDeactivate(CycleBack), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="420">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="0" Y="0" Z="0" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="0" Y="0" Z="0" />
      <Scale0 X="current" Y="current" Z="current" />
      <Scale1 X="*.33" Y="*.33" Z="*.33" />
      <Scale2 X="*.66" Y="*.66" Z="*.66" />
      <Scale3 X="1" Y="1" Z="1" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="160">
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="19.3" Y="0" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <AnimationDefinition Name="CycleForward" Panels="1,3" NextAnimation="CycleForwardWait" ExitEvents="PanelDeactivate(CycleBack), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="420">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="0" Y="0" Z="0" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="-33" Z="*.33" />
      <Rotation2 X="*.66" Y="-96" Z="*.66" />
      <Rotation3 X="0" Y="0" Z="0" />
      <Scale0 X="current" Y="current" Z="current" />
      <Scale1 X="*.33" Y="*.33" Z="*.33" />
      <Scale2 X="*.66" Y="*.66" Z="*.66" />
      <Scale3 X="1" Y="1" Z="1" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="160">
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="19.3" Y="0" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <AnimationDefinition Name="CycleForward" Panels="2,4" NextAnimation="CycleForwardWait" ExitEvents="PanelDeactivate(CycleBack), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="420">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="0" Y="0" Z="0" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="33" Z="*.33" />
      <Rotation2 X="*.66" Y="96" Z="*.66" />
      <Rotation3 X="0" Y="0" Z="0" />
      <Scale0 X="current" Y="current" Z="current" />
      <Scale1 X="*.33" Y="*.33" Z="*.33" />
      <Scale2 X="*.66" Y="*.66" Z="*.66" />
      <Scale3 X="1" Y="1" Z="1" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="160">
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="19.3" Y="0" Z="0" />
    </MotionPoints>
  </AnimationDefinition>

  <!-- Panel wait cycles -->
    
  <AnimationDefinition Name="CycleBackWait" Panels="0,1,2,3,4" NextAnimation="CycleBackWait" ExitEvents="PanelActivate(CycleForward), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Linear" DurationMS="30000">
    </MotionPoints>
  </AnimationDefinition>

  <AnimationDefinition Name="CycleForwardWait" Panels="0,1,2,3,4" NextAnimation="CycleForwardWait" ExitEvents="PanelDeactivate(CycleBack), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Linear" DurationMS="30000">
    </MotionPoints>
  </AnimationDefinition>
  
  <!-- Cycle for Panel 0 -->

  <AnimationDefinition Name="CycleBack" Panels="0" NextAnimation="CycleBackWait" ExitEvents="PanelActivate(CycleForward), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="220">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="current" Y="210" Z="current" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="0" Y="0" Z="0" />
    </MotionPoints>
    <MotionPoints TimeAdjust="Decelerated" DurationMS="520">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="current" Y="210" Z="450" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="-80" Y="0" Z="0" />
    </MotionPoints>
  </AnimationDefinition>
        
  <!-- Cycle for Panel 1 -->

  <AnimationDefinition Name="CycleBack" Panels="1" NextAnimation="CycleBackWait" ExitEvents="PanelActivate(CycleForward), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="220">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="-85" Y="0" Z="400" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="0" Y="-20" Z="0" />
      <Scale0 X="current" Y="current" Z="current" />
      <Scale1 X="*.33" Y="*.33" Z="*.33" />
      <Scale2 X="*.66" Y="*.66" Z="*.66" />
      <Scale3 X="0.5" Y="1" Z="1" />
    </MotionPoints>
  </AnimationDefinition>

  <!-- Cycle for Panel 2 -->

  <AnimationDefinition Name="CycleBack" Panels="2" NextAnimation="CycleBackWait" ExitEvents="PanelActivate(CycleForward), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="220">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="85" Y="0" Z="400" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="0" Y="20" Z="0" />
      <Scale0 X="current" Y="current" Z="current" />
      <Scale1 X="*.33" Y="*.33" Z="*.33" />
      <Scale2 X="*.66" Y="*.66" Z="*.66" />
      <Scale3 X="0.5" Y="1" Z="1" />
    </MotionPoints>
  </AnimationDefinition>

  <!-- Cycle for Panel 3 -->

  <AnimationDefinition Name="CycleBack" Panels="3" NextAnimation="CycleBackWait" ExitEvents="PanelActivate(CycleForward), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="220">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="-195" Y="0" Z="260" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="0" Y="-70" Z="0" />
      <Scale0 X="current" Y="current" Z="current" />
      <Scale1 X="*.33" Y="*.33" Z="*.33" />
      <Scale2 X="*.66" Y="*.66" Z="*.66" />
      <Scale3 X="0.5" Y="1" Z="1" />
    </MotionPoints>
  </AnimationDefinition>

  <!-- Cycle for Panel 4 -->

  <AnimationDefinition Name="CycleBack" Panels="4" NextAnimation="CycleBackWait" ExitEvents="PanelActivate(CycleForward), GameActivate(throw), GameDeactivate(throw)">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="220">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="0" Y="0" Z="*.33" />
      <Position2 X="0" Y="0" Z="*.66" />
      <Position3 X="195" Y="0" Z="260" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="0" Y="70" Z="0" />
      <Scale0 X="current" Y="current" Z="current" />
      <Scale1 X="*.33" Y="*.33" Z="*.33" />
      <Scale2 X="*.66" Y="*.66" Z="*.66" />
      <Scale3 X="0.5" Y="1" Z="1" />
    </MotionPoints>
  </AnimationDefinition>

  <!--
  When a new panel is selected, all existing panels should get thrown into the distance
  Note that since we don't have a NextAnimation, when this animation finishes, these objects
  will disappear from the scene.
  -->

  <AnimationDefinition Name="Throw" Panels="0,1,2,3,4">
    <MotionPoints TimeAdjust="Decelerated" DurationMS="2000">
      <Position0 X="current" Y="current" Z="current" />
      <Position1 X="*.33" Y="*.33" Z="*.33" />
      <Position2 X="*.66" Y="*.66" Z="*.66" />
      <Position3 X="rand(-1111, 1111)" Y="rand(10, 300)"  Z="1500" />
      <Rotation0 X="current" Y="current" Z="current" />
      <Rotation1 X="*.33" Y="*.33" Z="*.33" />
      <Rotation2 X="*.66" Y="*.66" Z="*.66" />
      <Rotation3 X="rand(-100, 100)" Y="rand(-45, 45)" Z="rand(-180, 180)" />
    </MotionPoints>
  </AnimationDefinition>

</ArrayOfAnimationDefinition>